    <thead>
      <!-- <th>Project</th> -->
      <th>User</th>
      <th>30 days Checklists</th>
      <th>60 days Checklists</th>
      <th>90+ day Checklists</th>
    </thead>  
    <tbody>
      <%# @projects.each do |project|%>
        <tr>
          <!-- <td><%#= @project.name %></td> -->
          <td><%= @uname %></td>
          <td> <%= link_to "#{@days_30}", project_reports_path(@project) %></td>
          <td><%= link_to "#{@days_60}", project_reports_path(@project) %></td>
          <td><%= link_to "#{@days_90}", project_reports_path(@project) %></td>
        </tr>
      <%# end %>   
    </tbody>  
